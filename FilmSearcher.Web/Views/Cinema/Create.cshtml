@model Cinema

@{
    ViewData["Title"] = "Create producer";
}

<form asp-action="Create" method="post">
    <div>
        <label>PictureURL</label>
        <input asp-for="Logo" type="text" />
    </div>
    <div>
        <label>Name</label>
        <input asp-for="Name" type="text" />
    </div>
    <div>
        <label>Description</label>
        <input asp-for="Description" type="text" />
    </div>
    <div>
        <input type="submit" value="Create" />
        <a asp-action="Cinemas">Show all</a>
    </div>
</form>
